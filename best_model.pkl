import pandas as pd
from sklearn.ensemble import RandomForestRegressor
import joblib

# Sample data
data = {
    'cement': [10, 20, 30, 40],
    'sand': [20, 30, 40, 50],
    'glass': [5, 10, 15, 20],
    'water': [200, 210, 220, 230],
    'strength': [30, 35, 40, 45]
}

df = pd.DataFrame(data)

# Features and target
X = df[['cement', 'sand', 'glass', 'water']]
y = df['strength']

# Train model
model = RandomForestRegressor()
model.fit(X, y)

# Save model
joblib.dump(model, 'best_model.pkl')
